{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "jasnross/fibonacci-docker-client",
      "hostname": "client",
      "essential": false,
      "memory": 256
    },
    {
      "name": "worker",
      "image": "jasnross/fibonacci-docker-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 256
    },
    {
      "name": "server",
      "image": "jasnross/fibonacci-docker-server",
      "hostname": "api",
      "essential": false,
      "memory": 256
    },
    {
      "name": "nginx",
      "image": "jasnross/fibonacci-docker-nginx",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["server", "client"]
    }
  ]
}
